{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["./imports/ui/**/*.{js,jsx}"],
      "extends": ["airbnb"],
      "rules": {
        "quotes": "off",
        "react/prop-types": "off",
        "object-curly-newline": "off",
        "import/no-unresolved": "off",
        "import/no-absolute-path": "off"
      }
    }
  ]
}
